{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Authentication": {
    "Audience": "account",
    "TokenValidationParameters": {
      "ValidIssuers": [
        "http://blog.identity:8080/realms/rommie",
        "http://localhost:18080/realms/rommie"
      ]
    },
    "MetadataAddress": "http://localhost:18080/realms/rommie/.well-known/openid-configuration",
    "RequireHttpsMetadata": false
  },
  "ReverseProxy": {
    "Routes": {
      "user-public": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "/users/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/users"
          }
        ],
        "AuthorizationPolicy": "Authenticated"
      },
      "user-protected": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "/users/api/v1/user/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/users"
          }
        ]
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "dest1": {
            "Address": "http://localhost:5001"
          }
        }
      }
    }
  },
  "AllowedHosts": "*"
}